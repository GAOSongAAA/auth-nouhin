<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.collaboportal.common.jwt.repository.AuthMapper">
    
    <select id="findUserMasterByEmail" parameterType="String" resultType="com.collaboportal.common.jwt.entity.UserMaster">
        SELECT							
            I_USER_TYPE as userType,
            I_USER_MAIL as userMail,
            I_PASSWORD as password
        FROM
            shohomoto_info.MS_RIYOUSYA
        WHERE
            I_USER_MAIL = #{userMail} 					
    </select>
</mapper>